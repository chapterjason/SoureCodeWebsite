{
    "version": "0.1.0-DEV",
    "strategies": [
        {
            "type": "expression",
            "options": {
                "pattern": ".env",
                "expression": "APP_VERSION={{SEMVER}}",
                "replacement": "APP_VERSION={{VERSION}}"
            }
        }
    ],
    "workflow": {
        "places": [
            "BETA"
        ],
        "transitions": {
            "MASTER": [
                "BETA"
            ],
            "BETA": [
                "BETA",
                "RELEASE"
            ]
        }
    }
}